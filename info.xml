<?xml version="1.0"?>
<extension key="com.civifirst.postcodesio" type="module">
  <file>postcodesio</file>
  <name>Postcodes.io</name>
  <description>Provides functions to look up UK address meta-data from Postcodes.io. Currently this looks up: the 'admin district' of an address, the geolocation, and the county. To use, go to a search results page, and look for Postcodes.io</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>John Kirk</author>
    <email>john@civifirst.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JohnFF/CiviCRM-Postcodes.io</url>
    <url desc="Documentation">https://github.com/JohnFF/CiviCRM-Postcodes.io</url>
    <url desc="Support">https://github.com/JohnFF/CiviCRM-Postcodes.io</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-08-07</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>Developed by CiviFirst, http://civifirst.com</comments>
  <civix>
    <namespace>CRM/Postcodesio</namespace>
  </civix>
</extension>
